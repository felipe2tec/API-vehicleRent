{
    "openapi":"3.0.0",
    "info":{
        "title":"VehiclesRent-api",
        "description":"Essa API tem como objetivo locar carros",
        "contact":{
            "email":"felipe2tec@gmail.com"
        },
        "version":"1.0.0"
    },
    "servers":[
        {
            "url":"http://127.0.0.1:3100",
            "description":"API de desenvolvimento"
        }
    ],
    "paths":{
        "/users":{
            "post":{
                "summary":"Cadastro de usuários",
                "description":"Rota responsável por cadastrar usuarios capazes de locar veículos",
                "tags":["Users"],
                "security":[{"bearerAuth":[]}],
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/User"
                            },
                            "examples":{
                                "user":{
                                    "value":{
                                        "email":"felipe2tec@gmail.com",
                                        "passwd":"password"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"OK",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "$ref":"#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            },
            "get":{
                "summary":"Consulta de usuários",
                "description":"Rota responsável por consultar os usuarios ",
                "tags":["Users"],
                "security":[{"bearerAuth":[]}],
                "responses":{
                    "200":{
                        "description":"OK",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "$ref":"#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            },
            "put":{
                "summary":"Alteração de usuários",
                "description":"Rota responsável por alterar um usuario",
                "tags":["Users"],
                "security":[{"bearerAuth":[]}],
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/User"
                            },
                            "examples":{
                                "user":{
                                    "value":{
                                        "email":"felipe2tec@gmail.com",
                                        "passwd":"password"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "204":{
                        "description":"Usuario alterado"
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "404":{
                        "description":"Usuario não existe"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            },
            "delete":{
                "summary":"Remoção de usuários",
                "description":"Rota responsável por remover um usuario",
                "tags":["Users"],
                "security":[{"bearerAuth":[]}],
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/User"
                            },
                            "examples":{
                                "user":{
                                    "value":{
                                        "email":"felipe2tec@gmail.com",
                                        "passwd":"password"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "204":{
                        "description":"Usuario alterado"
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "404":{
                        "description":"Usuario não existe"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            }
        },
        "/users/get_user":{
            "post":{
                "summary":"Procura por um  usuário",
                "description":"Rota responsável por consultar um usuarios",
                "tags":["Users"],
                "security":[{"bearerAuth":[]}],
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/User"
                            },
                            "examples":{
                                "user":{
                                    "value":{
                                        "email":"felipe2tec@gmail.com"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"OK",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "$ref":"#/components/schemas/User"
                                }
                            }
                        }
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            }
        },


        "/vehicles":{
            "post":{
                "summary":"Cadastro de veículos",
                "description":"Rota responsável por cadastrar veículos para locação",
                "tags":["Vehicles"],
                "security":[{"bearerAuth":[]}],
                "requestBody":{
                    "content":{
                        "application/json":{
                            "schema":{
                                "$ref":"#/components/schemas/VehicleCreate"
                            },
                            "examples":{
                                "user":{
                                    "value":{
                                        "model":"Opala",
                                        "plate":"1111111",
                                        "daily_price":"100,00"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses":{
                    "200":{
                        "description":"OK",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "$ref":"#/components/schemas/Vehicle"
                                }
                            }
                        }
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            },
            "get":{
                "summary":"Consulta de veículos",
                "description":"Rota responsável por consultar os veículos",
                "tags":["Vehicles"],
                "security":[{"bearerAuth":[]}],
                "responses":{
                    "200":{
                        "description":"OK",
                        "content":{
                            "application/json":{
                                "schema":{
                                    "type":"object",
                                    "$ref":"#/components/schemas/Vehicle"
                                },
                                "examples":{
                                    "user":{
                                        "value":{
                                            "model":"Opala",
                                            "plate":"1111111",
                                            "daily_price":"100,00",
                                            "rent":true,
                                            "init_date":"2022-05-15",
                                            "fisishe_date":"2022-05-25",
                                            "id_user":"2"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401":{
                        "description":"Sem autorização"
                    },
                    "500":{
                        "description":"Erro"
                    }
                }
            }
        }
    },
    "components":{
        "schemas":{
            "User":{
                "type":"object",
                "properties":{
                    "email":{
                        "type":"string"
                    },
                    "passwd":{
                        "type":"string"
                    }
                }
            },
            "VehicleCreate":{
                "type":"object",
                "properties":{
                    "plate":{
                        "type":"string"
                    },
                    "model":{
                        "type":"string"
                    },
                    "daily_price":{
                        "type":"number"
                    }
                }
            },
            "Vehicle":{
                "type":"object",
                "properties":{
                    "plate":{
                        "type":"string"
                    },
                    "model":{
                        "type":"string"
                    },
                    "daily_price":{
                        "type":"number"
                    },
                    "rent":{
                        "type":"boolean"
                    },
                    "init_date":{
                        "type":"string"
                    },
                    "fisinhe_date":{
                        "type":"string"
                    },
                    "id_user":{
                        "type":"number"
                    }
                }
            }
        },
        "securitySchemes":{
            "bearerAuth":{
                "type":"http",
                "scheme":"bearer",
                "bearerFormate":"JWT"
            }
        }
    }
}